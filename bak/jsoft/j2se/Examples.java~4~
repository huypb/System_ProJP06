public class Examples {

  public int getMax(int n, float m, char ch) {
    int max = n;

    if (max < (int) m) {
      max = (int) m;
    }

    if (max < ch) {
      max = ch;
    }
    return max;
  }


  public static void main(String[] args) {

    int n = (int) (Math.random() * 100);

    System.out.print("giá tr\u1ECB n = " + n);
  }
}
